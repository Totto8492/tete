{
    "version": "0.2.0",
    "configurations": [
        {
            "preLaunchTask": "rust: cargo build",
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "probe-rs Debug",
            "cwd": "${workspaceFolder}",
            "connectUnderReset": false,
            "chip": "STM32G031J6Mx",
            "flashingConfig": {
                "flashingEnabled": true,
                "haltAfterReset": false
            },
            "speed": 20000,
            "coreConfigs": [
                {
                    "coreIndex": 0,
                    "programBinary": "target/thumbv6m-none-eabi/debug/${workspaceFolderBasename}",
                    "chip": "STM32G031J6Mx",
                    "rttEnabled": true,
                }
            ]
        }
    ]
}